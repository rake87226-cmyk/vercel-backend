{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "src": "email.js",
      "src": "db.js",
      "src": "init_db.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "dest": "email.js",
      "dest": "db.js",
      "dest": "init_db.js"
    }
  ]
}
